<settings
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
  <mirrors>
    <mirror>
      <id>central-proxy</id>
      <mirrorOf>central</mirrorOf>
      <name>Local proxy of central repo</name>
      <url>https://mirror.hostco.ru/repository/maven-central</url>
    </mirror>
    <mirror>
      <id>maven-central-proxy</id>
      <mirrorOf>maven-central</mirrorOf>
      <name>Local proxy of central repo</name>
      <url>https://mirror.hostco.ru/repository/maven-central</url>
    </mirror>
    <mirror>
      <id>spring-milestones-proxy</id>
      <mirrorOf>spring-milestones</mirrorOf>
      <name>Local proxy of spring-milestones repo</name>
      <url>https://mirror.hostco.ru/repository/spring-milestones</url>
    </mirror>
  </mirrors>
  <servers>
    <server>
      <id>central-proxy</id>
      <password>${env.MAVEN_MIRROR_PASSWORD}</password>
      <username>${env.MAVEN_MIRROR_USER}</username>
    </server>
    <server>
      <id>maven-central-proxy</id>
      <password>${env.MAVEN_MIRROR_PASSWORD}</password>
      <username>${env.MAVEN_MIRROR_USER}</username>
    </server>
    <server>
      <id>spring-milestones-proxy</id>
      <password>${env.MAVEN_MIRROR_PASSWORD}</password>
      <username>${env.MAVEN_MIRROR_USER}</username>
    </server>
    <server>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${env.CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
      <id>gitlab-maven</id>
    </server>
    <server>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${env.CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
      <id>gitlab-maven-ovis</id>
    </server>
  </servers>
</settings>
